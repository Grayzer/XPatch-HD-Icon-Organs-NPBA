<!-- Harvest Everything! -->
<!-- Ogliss.Ykara.HarvestEverything -->
<!--
Leg
Foot
Arm
Hand
Stomach
Brain
*Heart
*Lung
*Kidney
*Liver
Eye
Nose
Ear
Jaw
-->
<Operation Class="PatchOperationFindMod">
<mods>
<li>Harvest Everything!</li>
</mods>
  <match Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
<!-- Harvest Everything! -->
<!-- Leg -->
<li Class="PatchOperationAdd">
    <xpath>/Defs/ThingDef[defName = "Leg"]</xpath>
    <value>
      <graphicData>
        <texPath>Things/Item/Health/Leg</texPath>
        <graphicClass>Graphic_StackCount</graphicClass>
        <color>(255, 255, 255)</color>
        <drawSize>1.00</drawSize>
      </graphicData>
    </value>
</li>

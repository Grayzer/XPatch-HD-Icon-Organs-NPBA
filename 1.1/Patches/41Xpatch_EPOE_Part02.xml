<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- ===xpath=== -->
<!-- Color Legend -->
<!--                    IconColor
  Natural               (255, 255, 255)
  Prostetic             (210, 166, 121)
  Syntetic              (255, 128, 128)
  Hydraulic             (51, 133, 255)
  Bionic                (51, 255, 173)
  Advanced Bionic       (0, 179, 60)
  Archotech             (255, 204, 0)
  Advanced Archotech    (204, 102, 0)
-->

<!-- EPOE
- Nature
+Jaw
- Prostetic
+SteelArm
+EyePatch
+HookHand
-->

<Operation Class="PatchOperationFindMod">
<mods>
<li>Expanded Prosthetics and Organ Engineering</li>
</mods>
  <match Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
<!-- Expanded Prosthetics and Organ Engineering -->
<!-- Ykara.EPOE -->
<!-- Nature -->
<!-- Leg -->

<!-- Jaw -->
<li Class="PatchOperationAdd">
    <xpath>/Defs/ThingDef[defName = "Jaw"]</xpath>
    <value>
      <graphicData>
        <texPath>Things/Item/Health/Jaw</texPath>
        <graphicClass>Graphic_StackCount</graphicClass>
        <color>(255, 255, 255)</color>
        <drawSize>1.00</drawSize>
      </graphicData>
    </value>
</li>
<li Class="PatchOperationAdd">
    <xpath>/Defs/HediffDef[defName = "Jaw"]</xpath>
    <value>
      <defaultLabelColor>(255, 255, 255)</defaultLabelColor>
    </value>
</li>
<!-- Prostetic -->
<!-- SteelArm -->
<li Class="PatchOperationAdd">
    <xpath>/Defs/ThingDef[defName = "SteelArm"]</xpath>
    <value>
      <graphicData>
        <texPath>Things/Item/Health/IronArm</texPath>
        <graphicClass>Graphic_StackCount</graphicClass>
        <color>(210, 166, 121)</color>
        <drawSize>1.00</drawSize>
      </graphicData>
    </value>
</li>
<li Class="PatchOperationAdd">
    <xpath>/Defs/HediffDef[defName = "SteelArm"]</xpath>
    <value>
      <defaultLabelColor>(210, 166, 121)</defaultLabelColor>
    </value>
</li>
<!-- EyePatch -->
<li Class="PatchOperationAdd">
    <xpath>/Defs/ThingDef[defName = "EyePatch"]</xpath>
    <value>
      <graphicData>
        <texPath>Things/Item/Health/XEyePatch</texPath>
        <graphicClass>Graphic_StackCount</graphicClass>
        <color>(210, 166, 121)</color>
        <drawSize>1.00</drawSize>
      </graphicData>
    </value>
</li>
<li Class="PatchOperationAdd">
    <xpath>/Defs/HediffDef[defName = "EyePatch"]</xpath>
    <value>
      <defaultLabelColor>(210, 166, 121)</defaultLabelColor>
    </value>
</li>
<!-- HookHand -->
<li Class="PatchOperationAdd">
    <xpath>/Defs/ThingDef[defName = "HookHand"]</xpath>
    <value>
      <graphicData>
        <texPath>Things/Item/Health/HookHand</texPath>
        <graphicClass>Graphic_StackCount</graphicClass>
        <color>(210, 166, 121)</color>
        <drawSize>1.00</drawSize>
      </graphicData>
    </value>
</li>
<li Class="PatchOperationAdd">
    <xpath>/Defs/HediffDef[defName = "HookHand"]</xpath>
    <value>
      <defaultLabelColor>(210, 166, 121)</defaultLabelColor>
    </value>
</li>

   </operations>
 </match>
</Operation>
</Patch>

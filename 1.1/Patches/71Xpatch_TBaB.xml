<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- ===xpath=== -->
<!-- Color Legend -->
<!--                    IconColor
  Natural               (255, 255, 255)
  Prostetic             (210, 166, 121)
  Syntetic              (255, 128, 128)
  Hydraulic             (51, 133, 255)
  Bionic                (51, 255, 173)
  Advanced Bionic       (0, 179, 60)
  Archotech             (255, 204, 0)
  Advanced Archotech    (204, 102, 0)
-->

<!-- The Birds and the Bees
- Bionic
BionicReproductiveOrgans


-->

<Operation Class="PatchOperationFindMod">
<mods>
<li>The Birds and the Bees</li>
</mods>
  <match Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
<!-- BionicReproductiveOrgans -->
<li Class="PatchOperationAdd">
    <xpath>/Defs/ThingDef[defName = "BionicReproductiveOrgans"]</xpath>
    <value>
      <graphicData>
        <texPath>Things/Item/Health/Reproductive</texPath>
        <graphicClass>Graphic_StackCount</graphicClass>
        <color>(51, 255, 173)</color>
        <drawSize>1.00</drawSize>
      </graphicData>
    </value>
</li>

<li Class="PatchOperationAdd">
    <xpath>/Defs/HediffDef[defName="BionicReproductiveOrgans"]</xpath>
    <value>
      <defaultLabelColor>(51, 255, 173)</defaultLabelColor>
    </value>
</li>

<li Class="PatchOperationAdd">
    <xpath>/Defs/HediffDef[defName="SimpleProstheticReproductiveOrgans"]</xpath>
    <value>
      <defaultLabelColor>(210, 166, 121)</defaultLabelColor>
    </value>
</li>

   </operations>
 </match>
</Operation>
</Patch>

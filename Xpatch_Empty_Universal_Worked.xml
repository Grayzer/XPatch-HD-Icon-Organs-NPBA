<Operation Class="PatchOperationSequence">
    <success>Normal</success>
    <operations>
      <li Class="PatchOperationTest">
        <xpath>Defs/ThingDef[defName="Empty"]</xpath>
        <success>Normal</success>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/ThingDef[defName="Empty"]/graphicData</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/ThingDef[defName = "Empty"]/graphicData</xpath>
        </match>
        <nomatch/>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[defName="Empty"]</xpath>
        <value>
          <graphicData>
            <texPath>Things/Item/Health/HealthItemMisc</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
            <color>(255, 255, 255)</color>
            <drawSize>1.00</drawSize>
          </graphicData>
        </value>
      </li>
    </operations>
</Operation>

<Operation Class="PatchOperationSequence">
    <success>Normal</success>
    <operations>
      <li Class="PatchOperationTest">
        <xpath>Defs/HediffDef[defName="Empty"]</xpath>
        <success>Normal</success>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>Defs/HediffDef[defName="Empty"]/defaultLabelColor</xpath>
        <match Class="PatchOperationRemove">
          <xpath>Defs/HediffDef[defName = "Empty"]/defaultLabelColor</xpath>
        </match>
        <nomatch/>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>Defs/HediffDef[defName="Empty"]</xpath>
        <value>
          <defaultLabelColor>(255, 255, 255)</defaultLabelColor>
        </value>
      </li>
    </operations>
</Operation>
<!-- Empty -->



</Patch>

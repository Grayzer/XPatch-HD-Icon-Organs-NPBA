<!-- ===xpath=== -->
<!-- Color Legend -->
<!--                    IconColor
  Natural               (255, 51, 153)  (0.5, 0.8, 0.5)
  Prostetic             (255, 153, 102)
  Syntetic              (102, 255, 51)
  Hydraulic             (102, 102, 153)
  Bionic                (51, 153, 255)
  Advanced Bionic       (0, 77, 153)
  Archotech             (230, 77, 0)
  Advanced Archotech    (230, 0, 0)
-->

<!-- RimWorld -->
<!-- Natural BodyPartNaturalBase -->
  <!--
  +Heart    [No HediffDef]
  +Lung     [No HediffDef]
  +Kidney   [No HediffDef]
  +Liver    [No HediffDef]
  -->
<!-- Prostetic BodyPartProstheticBase -->
  <!--
  +SimpleProstheticLeg
  +SimpleProstheticArm
  +SimpleProstheticHeart
  +CochlearImplant
  +Denture    [Only HediffDef]
  +PegLeg     [Only HediffDef]
  +WoodenFoot [Only HediffDef]
  -->
<!-- Bionic BodyPartBionicBase BodyPartArtificialBase -->

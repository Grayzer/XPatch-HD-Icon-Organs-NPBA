  +BionicEye
  +BionicArm
  +BionicLeg
  +BionicSpine
  +BionicHeart
  +BionicStomach
  +BionicEar
  +PowerClaw
  +Joywire
  +Painstopper
  -->
<!-- Archotech BodyPartArchotechBase -->
  <!--
  +ArchotechEye
  +ArchotechArm
  +ArchotechLeg
  -->

<!-- Vanilla -->
<!-- Remove default graph -->
      <Operation Class="PatchOperationRemove">
        <xpath>Defs/ThingDef[@Name = "BodyPartNaturalBase"]/graphicData</xpath>
      </Operation>

      <Operation Class="PatchOperationRemove">
        <xpath>Defs/ThingDef[@Name = "BodyPartProstheticBase"]/graphicData</xpath>
      </Operation>

      <Operation Class="PatchOperationRemove">
        <xpath>Defs/ThingDef[@Name = "BodyPartBionicBase"]/graphicData</xpath>
      </Operation>

      <Operation Class="PatchOperationRemove">
        <xpath>Defs/ThingDef[@Name = "BodyPartArchotechBase"]/graphicData</xpath>
      </Operation>

      <Operation Class="PatchOperationRemove">
        <xpath>Defs/HediffDef[@Name = "AddedBodyPartBase"]/defaultLabelColor</xpath>
      </Operation>

      <Operation Class="PatchOperationRemove">
        <xpath>Defs/HediffDef[@Name = "ImplantHediffBase"]/defaultLabelColor</xpath>
      </Operation>

<!-- Add new graph (IconColor) -->
      <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[@Name = "BodyPartNaturalBase"]</xpath>
        <value>
          <graphicData>
            <graphicClass>Graphic_StackCount</graphicClass>
            <color>(0.5, 0.8, 0.5)</color>
            <drawSize>1.00</drawSize>
          </graphicData>
        </value>
      </Operation>

      <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[@Name = "BodyPartProstheticBase"]</xpath>

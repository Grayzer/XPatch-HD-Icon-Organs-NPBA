<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- ===xpath=== -->
<!-- Color Legend -->
<!--                    IconColor
  Natural               (255, 255, 255)
  Prostetic             (210, 166, 121)
  Syntetic              (255, 128, 128)
  Hydraulic             (51, 133, 255)
  Bionic                (51, 255, 173)
  Advanced Bionic       (0, 179, 60)
  Archotech             (255, 204, 0)
  Advanced Archotech    (204, 102, 0)
-->

<!-- EvolvedOrgansRedux -->
<!--
- Royalty
?EVOR_Item_Skull_Resonance  пси усилитель черепа
?EVOR_Item_Artifact_Royalty_NornCrown симбионт в грудину меняющий настроение окружающих
?EVOR_Item_Artifact_Royalty_ReactiveWorms черви переводят в стелс при боли
-->

<Operation Class="PatchOperationFindMod">
<mods>
<li>EvolvedOrgansRedux</li>
</mods>
  <match Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
<!-- Royalty -->
<!-- EVOR_Item_Skull_Resonance -->
      <li Class="PatchOperationConditional">
        <xpath>/Defs/ThingDef[defName = "EVOR_Item_Skull_Resonance"]/graphicData</xpath>
        <match Class="PatchOperationReplace">
          <xpath>/Defs/ThingDef[defName = "EVOR_Item_Skull_Resonance"]/graphicData</xpath>
          <value>
            <graphicData>
              <texPath>Things/Item/Health/SkullResonance</texPath>
              <graphicClass>Graphic_StackCount</graphicClass>
              <color>(255, 128, 128)</color>
              <drawSize>1.00</drawSize>
            </graphicData>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName = "EVOR_Item_Skull_Resonance"]</xpath>
          <value>
            <graphicData>
              <texPath>Things/Item/Health/SkullResonance</texPath>
              <graphicClass>Graphic_StackCount</graphicClass>
              <color>(255, 128, 128)</color>
              <drawSize>1.00</drawSize>
            </graphicData>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>/Defs/HediffDef[defName = "EVOR_Hediff_Skull_Resonance"]/defaultLabelColor</xpath>
        <match Class="PatchOperationReplace">
          <xpath>/Defs/HediffDef[defName = "EVOR_Hediff_Skull_Resonance"]/defaultLabelColor</xpath>
          <value>
            <defaultLabelColor>(255, 128, 128)</defaultLabelColor>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>/Defs/HediffDef[defName = "EVOR_Hediff_Skull_Resonance"]</xpath>
          <value>
            <defaultLabelColor>(255, 128, 128)</defaultLabelColor>
          </value>
        </nomatch>
      </li>
<!-- EVOR_Item_Artifact_Royalty_NornCrown -->
      <li Class="PatchOperationConditional">
        <xpath>/Defs/ThingDef[defName = "EVOR_Item_Artifact_Royalty_NornCrown"]/graphicData</xpath>
        <match Class="PatchOperationReplace">

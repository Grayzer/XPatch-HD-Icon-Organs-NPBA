          <value>
            <defaultLabelColor>(255, 128, 128)</defaultLabelColor>
          </value>
        </nomatch>
      </li>
<!-- EVOR_Item_Artifact_Royalty_ReactiveWorms -->
      <li Class="PatchOperationConditional">
        <xpath>/Defs/ThingDef[defName = "EVOR_Item_Artifact_Royalty_ReactiveWorms"]/graphicData</xpath>
        <match Class="PatchOperationReplace">
          <xpath>/Defs/ThingDef[defName = "EVOR_Item_Artifact_Royalty_ReactiveWorms"]/graphicData</xpath>
          <value>

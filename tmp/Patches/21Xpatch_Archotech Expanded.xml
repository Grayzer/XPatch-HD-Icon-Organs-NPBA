<!-- ===xpath=== -->
<!-- Color Legend -->
<!--                    IconColor
  Natural               (255, 255, 255)
  Prostetic             (210, 166, 121)
  Syntetic              (255, 128, 128)
  Hydraulic             (51, 133, 255)
  Bionic                (51, 255, 173)
  Advanced Bionic       (0, 179, 60)
  Archotech             (255, 204, 0)
  Advanced Archotech    (204, 102, 0)
-->

<!-- Archotech Expanded -->
<!-- Archotech -->
  <!-- Implant -->

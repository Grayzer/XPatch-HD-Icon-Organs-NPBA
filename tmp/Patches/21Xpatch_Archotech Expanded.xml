<!-- Implant -->
<!-- ArchotechBrainMelder -->
      <li Class="PatchOperationConditional">
        <xpath>/Defs/ThingDef[defName = "ArchotechBrainMelder"]/graphicData</xpath>
        <match Class="PatchOperationReplace">
          <xpath>/Defs/ThingDef[defName = "ArchotechBrainMelder"]/graphicData</xpath>
          <value>
            <graphicData>
              <texPath>Things/Item/Health/BrainMelder</texPath>
              <graphicClass>Graphic_StackCount</graphicClass>
              <color>(255, 204, 0)</color>
              <drawSize>1.00</drawSize>

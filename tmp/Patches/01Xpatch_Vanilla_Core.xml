<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- ===xpath=== -->
<!-- Color Legend -->
<!--                    IconColor
  Natural               (255, 255, 255)
  Prostetic             (210, 166, 121)
  Syntetic              (255, 128, 128)
  Hydraulic             (51, 133, 255)
  Bionic                (51, 255, 173)
  Advanced Bionic       (0, 179, 60)
  Archotech             (255, 204, 0)
  Advanced Archotech    (204, 102, 0)
-->
<!-- RimWorld Core-->
<!-- Luden.RimWorld -->
<!--
- Natural
+Heart
+Lung
+Kidney
+Liver
- Prostetic
+SimpleProstheticLeg
+SimpleProstheticArm
+SimpleProstheticHeart
+CochlearImplant
- Bionic
+BionicEye
+BionicArm
+BionicLeg
+BionicSpine
+BionicHeart
+BionicStomach
+BionicEar
+PowerClaw
+DeathAcidifier
+Joywire
+Painstopper
- Archotech
+ArchotechEye
+ArchotechArm
+ArchotechLeg
-->
<Operation Class="PatchOperationConditional">
  <xpath>/Defs/ThingDef[defName = "PsychicSilencer"]</xpath>
  <nomatch Class="PatchOperationSequence">
    <success>Normal</success>
    <operations>
      <li Class="PatchOperationRemove">
        <xpath>/Defs/HediffDef[@Name = "ImplantHediffBase"]/defaultLabelColor</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>/Defs/ThingDef[@Name = "BodyPartNaturalBase"]/graphicData</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>/Defs/ThingDef[@Name = "BodyPartProstheticBase"]/graphicData</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>/Defs/ThingDef[@Name = "BodyPartBionicBase"]/graphicData</xpath>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>/Defs/ThingDef[@Name = "BodyPartArchotechBase"]/graphicData</xpath>
      </li>

<!-- Nature -->
<!-- Heart -->
<li Class="PatchOperationAdd">
  <xpath>/Defs/ThingDef[defName = "Heart"]</xpath>
  <value>
    <graphicData>
      <texPath>Things/Item/Health/Heart</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <color>(255, 255, 255)</color>
      <drawSize>1.00</drawSize>
    </graphicData>
  </value>
</li>
